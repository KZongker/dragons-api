{
  "name": "dragons-api",
  "version": "1.0.0",
  "description": "Node api for dragon data fetching practice",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch source -e ts,ejs --exec npm run dev:start",
    "dev:start": "npm-run-all build startDebug",
    "dev:watch": "npm run tsc-watch --onSuccess \"node build/index.js\"",
    "build": "npm-run-all clean lint tsc",
    "startDebug": "node --inspect=0.0.0.0:9229 .",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc"
  },
  "author": "Katrina Zongker",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^5.0.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^3.1.7",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/mysql": "^2.15.26",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tslint": "^6.1.3"
  }
}